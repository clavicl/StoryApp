<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fsu.mobile.storyapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.fsu.mobile.storyapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="CreateStoryActivity"
            android:label="@string/createStoryTitle"
            android:parentActivityName="com.fsu.mobile.storyapp.MainActivity.java">
        </activity>
        <activity
            android:name="CheckCreatedActivity"
            android:label="@string/checkCreatedTitle"
            android:parentActivityName="com.fsu.mobile.storyapp.MainActivity.java">
        </activity>
        <activity
            android:name="CheckContributedActivity"
            android:label="@string/checkContributedTitle"
            android:parentActivityName="com.fsu.mobile.storyapp.MainActivity.java">
        </activity>
        <activity
            android:name="CheckRequestActivity"
            android:label="@string/checkRequestTitle"
            android:parentActivityName="com.fsu.mobile.storyapp.MainActivity.java">
        </activity>
    </application>
</manifest>
